    "version": "0.2.0",
    "configurations": [

        
        {
            "type": "node",
            "request": "launch",
            "name": "backend server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["${workspaceFolder}/server/src/script.ts"],
            "console": "integratedTerminal",
            "runtimeArgs": ["--import", "tsx"],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "envFile": "${workspaceFolder}/server/.env"
        }
    ]
}


